@model IEnumerable<IntegradorP.Models.UsuarioPerfil>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Empresa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Perfil)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuario)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Empresa.EmpresaId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Perfil.PerfilId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Usuario.UsuarioId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Upid">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Upid">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Upid">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
